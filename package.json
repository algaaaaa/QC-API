{
  "name": "qc-api",
  "version": "1.0.0",
  "description": "API to fetch product images from Doppel and transform them",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:watermark": "node server-with-watermark.js",
    "start:production": "NODE_ENV=production node production-server.js",
    "dev": "nodemon server.js",
    "dev:watermark": "nodemon server-with-watermark.js",
